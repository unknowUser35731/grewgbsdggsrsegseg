	getScreenSize()
	changeElementsUi()
	makeFirstElements()
	guiChanges()
	createFont()
	controlePosition()
	getFunctionsStates()
	keybindsAnimations()
	keybindsBase()
	cefasdfv()
end)
--@endregion
